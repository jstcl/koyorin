{
    "name": "koyori",
    "version": "4.0.0",
    "description": "ðŸ§ª Maid and Helper Discord Bot for My Private Discord Server",
    "main": "index.js",
    "private": true,
    "scripts": {
        "start": "ts-node --transpile-only ./src/index.ts",
        "dev": "ts-node-dev --respawn --clear --rs --transpile-only --poll ./src/index.ts",
        "ci": "ts-node --transpile-only ./src/Bin/CI.ts",
        "build": "ts-node --transpile-only ./src/Bin/Build.ts",
        "build:native": "tsc",
        "lint": "eslint .",
        "format": "prettier -w .",
        "check-format": "prettier -c ."
    },
    "author": "Gifaldy Azka",
    "license": "Apache-2.0",
    "dependencies": {
        "cabul": "^0.0.15",
        "chalk": "^4.1.2",
        "color-thief-node": "^1.0.4",
        "consola": "^2.15.3",
        "danbooru": "^3.1.0",
        "discord.js": "13.3.1",
        "dotenv": "^10.0.0",
        "glob": "^7.2.0",
        "holodex.js": "^1.1.0",
        "moment": "^2.29.1",
        "mongoose": "6.1.2",
        "node-fetch": "2.6.6",
        "pretty-ms": "^7.0.1",
        "rss-parser": "^3.12.0",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^15.0.0",
        "@commitlint/config-angular": "^15.0.0",
        "@types/node": "^17.0.1",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^8.5.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.3",
        "prettier": "^2.5.1"
    },
    "engines": {
        "node": "16.x",
        "yarn": "1.x"
    },
    "keywords": [
        "bot",
        "discord",
        "discord-js",
        "nsfw",
        "hololive",
        "koyorin",
        "holo-x"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.ts": "prettier --write",
        "*.js": "prettier --write"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-angular"
        ],
        "rules": {
            "scope-case": [
                2,
                "always",
                "pascal-case"
            ],
            "type-enum": [
                2,
                "always",
                [
                    "chore",
                    "build",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test"
                ]
            ]
        }
    }
}
